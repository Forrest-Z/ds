#include<iostream>
#include <ros/ros.h>
#include "ivactuator/ivactuator.h"
#include <string>

using namespace std;



int main(int argc, char **argv) {
    ros::init(argc, argv, "ivsimuact");
	ros::NodeHandle nh;
    ros::Rate rate ( 20 );
    

    ros::Publisher pub = nh.advertise < ivactuator::ivactuator > ( "ivactuator", 1000  );
    ivactuator::ivactuator curact;
    while(ros::ok())
    {
        rate.sleep();
 
        nh.getParam("cur_sim_actspeed", curact.uispeed);
     

        pub.publish(curact);
    }
      return 0;

}









